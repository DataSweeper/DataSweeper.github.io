<table cellpadding="0" cellspacing="0" border="0" width="100%" class="table">
	<tr>
		<th>iplist</th>
		<th>portlist</th>
		<th>proto</th>
		<th>name</th>
	</tr>
{{#each model.data as |item|}}
	<tr>
	<td>{{item.attributes.iplist}}</td>
	<td>{{item.attributes.portlist}}</td>
	<td>{{item.attributes.protocol}}</td>
	<td>{{item.attributes.name}}</td>
	<td><button type="button" class="btn btn-info" {{action 'handlenmap' item.id on='click'}}>Run</button></td>
	</tr>
{{/each}}
</table>
